<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="hamonikr-chatbot">
    <schema id="@APP_ID@" path="/org/hamonikr/Chatbot/">
		<key name="local-mode" type="b">
			<default>false</default>
		</key>
		<key name="current-provider" type="s">
			<default>'ollama'</default>
		</key>
		<key name="model" type="s">
			<default>'orca-mini-3b.ggmlv3.q4_0.bin'</default>
		</key>
		<key name="bot-name" type="s">
			<default>'Assistant'</default>
		</key>
		<key name="user-name" type="s">
			<default>'User'</default>
		</key>
		<key name="width" type="i">
			<default>800</default>
		</key>
		<key name="height" type="i">
			<default>600</default>
		</key>
		<key name="is-maximized" type="b">
			<default>false</default>
		</key>
		<key name="is-fullscreen" type="b">
			<default>false</default>
		</key>
		<key name="color-scheme" type="s">
			<default>'light'</default>
		</key>
		<key name="chat-font-family" type="s">
			<default>'Sans'</default>
		</key>
		<key name="chat-font-size" type="i">
			<default>11</default>
		</key>
		<key name="chat-line-height" type="d">
			<default>1.6</default>
		</key>

	</schema>
</schemalist>
